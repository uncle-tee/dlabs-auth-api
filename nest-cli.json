{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": [
      {
        "name": "@nestjs/swagger/plugin",
        "options": {
          "dtoFileNameSuffix": [
            "Dto.ts"
          ],
          "controllerFileNameSuffix": [
            "Controller.ts"
          ]
        }
      }
    ]
  }
}
